@model Emp_Demo.Models.Userlogin
@{
    ViewBag.Title = "Login";
  

}
@*<script type="text/javascript">
    function preventBack() { window.history.forward(); }
    setTimeout("preventBack()", 0);
    window.onunload = function () { null };
</script>*@

<style>
    body {
        overflow: hidden;
        background-color: black;
    }

    label {
        display: -webkit-box;
    }
</style>
<section class="vh-100" style="background-color: black;">

    <div class="container py-4 h-60">
        <div class="row d-flex justify-content-center align-items-center ">
            <div class="col-12 col-md-8 col-lg-6 col-xl-5">
                <div class="card shadow-2-strong" style="border-radius: 1rem;">
                    <div class="card-body p-5 text-center ">
                        <img src="data:image/jpeg;base64," alt="Logo" style="width:40px;" class="rounded-pill">
                        <h3 class="mb-5">LogIn</h3>
                        @using (Html.BeginForm("Login", "Account", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
                         {
                            @Html.AntiForgeryToken()
                            <div class="form-group">
                                @Html.LabelFor(m => m.Username, "User Name ",new { @class = "control-label col-md-3" })
                                <div class="col-md-10">
                                    <div data-mdb-input-init class="form-outline mb-4">
                                        @Html.TextBoxFor(m => m.Username, new { @class = "form-control form-control-lg", placeholder = "Enter a valid user name" })
                                  
                                    </div>
                               
                                </div>
                            </div>
                            <div class="form-group">
                                @Html.LabelFor(m => m.Password, new { @class = "control-label col-md-3" })
                                <div class="col-md-10">
                                    <div data-mdb-input-init class="form-outline mb-4">
                                        @Html.PasswordFor(m => m.Password, new { @class = "form-control form-control-lg", placeholder = "Enter a valid password" })
                                 
                                    </div>
                                    @*@Html.ValidationMessageFor(m => m.Password, "", new { @class = "text-danger" })*@
                                </div>
                            </div>


                            <div class="text-center text-lg-start mt-4 pt-2">
                                <button type="submit" class="btn btn-primary btn-lg"
                                        style="padding-left: 2.5rem; padding-right: 2.5rem;">
                                    Login
                                </button>
                            </div>

        


                         }

                    </div>


                </div>
            </div>
        </div>
    </div>

</section>
  




